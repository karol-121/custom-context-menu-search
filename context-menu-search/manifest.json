{

  "manifest_version": 2,
  "name": "Custom context menu seach",
  "description": "Customizable open in new tab from context menu.",
  "version": "1.0",
  
  "icons": {
    "48": "icons/icon-48.png"
  },

  "background": {
    "scripts":["background/context-menu.js"]
  },

  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },

  "permissions": [
    "storage", 
    "tabs",
    "contextMenus"
  ]

}
