{

  "manifest_version": 2,
  "name": "Custom context menu searches",
  "description": "Customizable context menu search items.",
  "version": "1.0",
  
  "icons": {
    "48": "icons/icon-48.png"
  },

  "background": {
    "scripts":["utils/input-validation.js","background/context-menu.js"]
  },

  "options_ui": {
    "page": "gui/page.html",
    "browser_style": true
  },

  "permissions": [
    "storage", 
    "tabs",
    "contextMenus"
  ]

}
